Config { font = "xft:Bitstream Vera Sans Mono:size=9:bold:antialias=true"  
     , bgColor = "#000000"  
     , fgColor = "grey"  
    -- , position = Static { xpos = 0 , ypos = 0, width = 1290, height = 16 }  
     , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10  
             , Run Network "enp0s3" ["-L","0","-H","100","--normal","green","--high","orange"] 10   
             , Run Memory ["-t","Mem: <usedratio>%"] 10  
             , Run Date "%l:%M" "date" 10  
             , Run StdinReader  
           ]  
     , sepChar = "%"  
     , alignSep = "}{"  
     , template = " %StdinReader%}{ <fc=grey>%cpu%</fc><fc=grey>  %memory%  %enp0s3%</fc>"  
     }
